{
  "name": "YouTools",
  "description": "Extension for YouTube",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "youtools.html",
    "default_icon": "images/youtools-gray.png"
  }
}
